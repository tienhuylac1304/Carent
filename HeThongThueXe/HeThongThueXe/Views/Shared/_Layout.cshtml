            <div class="site-mobile-menu-body"></div>
        </div>*@



        <header class="site-navbar site-navbar-target navigation" role="banner">

            <div class="container container1">
                <div class="row align-items-center position-relative">

                    <div class="col-3 ">
                        <div class="site-logo">
                            @if (Session["LoaiUser"] == null)
                            {
                                <h1>@Html.ActionLink("CarRent", "Index", "Home")</h1>
                            }
                            else if (Session["LoaiUser"].ToString() == "1")
                            {
                                <h1>@Html.ActionLink("CarRent", "Index", "AdminCarViewModels")</h1>
                            }
                            else
                            {
                                <h1>@Html.ActionLink("CarRent", "Index", "Home")</h1>
                            }

                        </div>
                    </div>

                    <div class="col-9  text-right">
                        <span class="d-inline-block d-lg-none">
                            <a href="#" class="text-white site-menu-toggle js-menu-toggle py-5 text-white">
                                <span class="icon-menu h3 text-white"></span>
                            </a>
                        </span>

                    <nav class="site-navigation text-right ml-auto d-none d-lg-block" role="navigation">
                        <ul class="site-menu  js-clone-nav ml-auto " style="font-size:25px">
                            <li><a href="@Url.Action("Index", "Home")" class="nav-link">Home</a></li>
                            <li><a href="@Url.Action("Index", "Car")" class="nav-link">Cars</a></li>
                            <li><a href="@Url.Action("Index", "About")" class="nav-link">About</a></li>
                            @*<li ><a href="@Url.Action("Index", "Contact")" class="nav-link">Contact</a></li>*@
                        <li style="margin-left:50px; float:right">
                            @if (Session["NameUser"] != null)
                            {

                               <a href="@Url.Action("Index", "AccountInfo")">@Session["NameUser"]</a>
                            }
                            else
                            {

                                <a href="@Url.Action( "Login", "Login")">Login</a>;
                            }
                        </li>
                        </ul>
                    </nav>                        
                    </div>


                </div>
            </div>

        </header>
            @RenderBody()
    <div style="position:absolute;margin-top:30px;display:block;width:100%">
        <hr />
        <footer style="text-align:right; margin-right:10px">
            <p>&copy; @DateTime.Now.Year - CarRent</p>
        </footer>
    </div>

    @*</div>*@

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
